@model IEnumerable<MateriaaliVarasto.Models.Valmistajat>

@{
    ViewBag.Title = "Index";
}

<h2 style="padding-top: 1em">Valmistajat</h2>

<br />
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Valmistaja)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Valmistaja)
        </td>
    </tr>
}

</table>
